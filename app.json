{
  "expo": {
    "name": "Sjurados",
    "slug": "Sjurados",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "sjurados",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#3D3943"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#3D3943"
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "sjurados",
              "host": "*",
              "pathPrefix": "/"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "package": "com.maarin_lucas.Sjurados"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./app/imagens/logo.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "008c9433-8174-411a-af35-e61d740915e7"
      }
    },
    "runtimeVersion": "1.0.0",  
    "updates": {
      "url": "https://u.expo.dev/008c9433-8174-411a-af35-e61d740915e7"
    }
  }
}
